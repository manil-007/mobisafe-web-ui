{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://api.driversaathi.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/",
      "status": 200
    }
  ]
}
